doctype html
html(lang="en")
  block head
    head
      meta(charset="utf-8")
      meta(name="description" content="Evoke Health descrip")
      meta(content="width=device-width, initial-scale=1" name="viewport")
      meta(content="ie=edge" http-equiv="x-ua-compatible")

      link(rel="icon" type="image/x-icon" href="assets/favicon/favicon.ico")

      link(href="assets/fonts/Poppins-Bold.woff2" as="font" rel="preload" type="font/woff2" crossorigin="anonymous")
      link(href="assets/fonts/Poppins-Regular.woff2" as="font" rel="preload" type="font/woff2" crossorigin="anonymous")

      link(href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet")

      link(rel="stylesheet" href="assets/css/style.min.css")

      block title
        title Evoke Health
  body
    include ../mixins/common
    .page
      block header
        include header
      block content
      block footer
        include footer
      #overlay.overlay
    block scripts
      script(src="https://cdn.jsdelivr.net/npm/swiper@8/swiper-bundle.min.js")
      script(src="https://unpkg.com/aos@2.3.1/dist/aos.js")
      script(src='assets/js/main.js')
